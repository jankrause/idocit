<list>
  <thematicGrid>
    <name>Converting Operations</name>
    <description>A converting operation transforms an OBJECT from the SOURCE_FORMAT into the DESTINATION_FORMAT by using an INSTRUMENT, ALGORITHM or RULE. This category bases on the VerbNet classes convert-26.6.2 and Turn-26.6.1</description>
    <verbs>
      <string>get around</string>
      <string>change</string>
      <string>convert</string>
      <string>switch</string>
      <string>go back</string>
      <string>come around</string>
      <string>transform</string>
      <string>revert</string>
      <string>deform</string>
      <string>resort</string>
      <string>switch_over</string>
      <string>turn</string>
      <string>get down</string>
      <string>get</string>
      <string>metamorphose</string>
      <string>return</string>
      <string>settle down</string>
      <string>alter</string>
      <string>take</string>
      <string>fall</string>
      <string>move over</string>
      <string>translate</string>
      <string>transmute</string>
      <string>change_over</string>
      <string>shift</string>
      <string>mutate</string>
    </verbs>
    <roles>
      <entry>
        <thematicRole>
          <name>INSTRUMENT</name>
          <description>The entity with which or through which an action is performed</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>false</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>SOURCE_FORMAT</name>
          <description>The FORMAT of the input(s) of an ACTION</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>RULE</name>
          <description>Defines how decisions were made by the ACTION</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>false</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>DESTINATION_FORMAT</name>
          <description>The FORMAT of the output(s) of an ACTION</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>ACTION</name>
          <description>A function performed by the component</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>OBJECT</name>
          <description>The object, target or result of an action. It can also reference the system implementing a described functionality</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>ALGORITHM</name>
          <description>Set of instructions to perform the ACTION</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>false</boolean>
      </entry>
    </roles>
    <gridBasedRules>
      <entry>
        <string>RULE</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>DESTINATION_FORMAT</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>OBJECT</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>INSTRUMENT</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>SOURCE_FORMAT</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>ALGORITHM</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>ACTION</string>
        <string>always();</string>
      </entry>
    </gridBasedRules>
  </thematicGrid>
  <thematicGrid>
    <name>Mathematical Operations</name>
    <description>A mathematical operation computes the OBJECT by applying a mathematical FORMULA to one or more OPERANDs. In case of many OBJECTs, their ORDERING should be specified. This category bases on the VerbNet class multiply-108.</description>
    <verbs>
      <string>inerpolate</string>
      <string>count</string>
      <string>calculate</string>
      <string>extrapolate</string>
      <string>deduct</string>
      <string>sum</string>
      <string>divide</string>
      <string>multiply</string>
      <string>add</string>
      <string>subtract</string>
      <string>tally</string>
      <string>factor out</string>
      <string>compute</string>
      <string>average</string>
    </verbs>
    <roles>
      <entry>
        <thematicRole>
          <name>NAME</name>
          <description>Non-numerical identifier of the OBJECT</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>INSTRUMENT</name>
          <description>The entity with which or through which an action is performed</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>ORDERING</name>
          <description>The ordering of the returned OBJECTs</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>false</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>SOURCE_FORMAT</name>
          <description>The FORMAT of the input(s) of an ACTION</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>FORMULA</name>
          <description>Formula used to compute a value</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>ALGORITHM</name>
          <description>Set of instructions to perform the ACTION</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>OPERAND</name>
          <description>Argument of the ACTION</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
    </roles>
    <gridBasedRules>
      <entry>
        <string>AGENT</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>NAME</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>FORMULA</string>
        <string>!exists(&quot;ACTION&quot;)</string>
      </entry>
      <entry>
        <string>OBJECT</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>ORDERING</string>
        <string>isPlural(&quot;OBJECT&quot;)</string>
      </entry>
      <entry>
        <string>INSTRUMENT</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>PRIMARY_KEY</string>
        <string>isPlural(&quot;OPERAND&quot;) &amp;&amp;
					!exists(&quot;COMPARISON&quot;)</string>
      </entry>
      <entry>
        <string>ALGORITHM</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>SOURCE_FORMAT</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>ACTION</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>COMPARISON</string>
        <string>isPlural(&quot;OPERAND&quot;) &amp;&amp;
					!exists(&quot;PRIMARY_KEY&quot;)</string>
      </entry>
      <entry>
        <string>OPERAND</string>
        <string>always();</string>
      </entry>
    </gridBasedRules>
  </thematicGrid>
  <thematicGrid>
    <name>Searching Operations</name>
    <description>A searching operation searches for one or many OBJECTs at SOURCE. The searched OBJECTs are identified by one or many COMPARISONs or PRIMARY_KEYs. The number of found OBJECTs could be limited by specifying a LIMIT. In case of many OBJECTs an ORDERING for them could be specified. The ALGORITHM defines the way the OBJECTs are searched. This category bases on the VerbNet classes Search-35.2 and obtain-13.5.2.</description>
    <verbs>
      <string>patrol</string>
      <string>shop</string>
      <string>grab</string>
      <string>scavenge</string>
      <string>advertise</string>
      <string>rake</string>
      <string>accept</string>
      <string>seize</string>
      <string>prospect</string>
      <string>purchase</string>
      <string>recover</string>
      <string>probe</string>
      <string>excavate</string>
      <string>search</string>
      <string>exact</string>
      <string>receive</string>
      <string>dredge</string>
      <string>find</string>
      <string>scour</string>
      <string>acquire</string>
      <string>scout</string>
      <string>appropriate</string>
      <string>check</string>
      <string>get</string>
      <string>watch</string>
      <string>select</string>
      <string>drag</string>
      <string>sift</string>
      <string>dive</string>
      <string>inherit</string>
      <string>comb</string>
      <string>borrow</string>
      <string>snatch</string>
      <string>cadge</string>
      <string>trawl</string>
      <string>obtain</string>
      <string>quarry</string>
      <string>troll</string>
      <string>regain</string>
      <string>retrieve</string>
      <string>plumb</string>
      <string>prowl</string>
      <string>rifle</string>
      <string>accrue</string>
      <string>accumulate</string>
      <string>collect</string>
    </verbs>
    <roles>
      <entry>
        <thematicRole>
          <name>INSTRUMENT</name>
          <description>The entity with which or through which an action is performed</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>false</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>ORDERING</name>
          <description>The ordering of the returned OBJECTs</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>false</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>COMPARISON</name>
          <description>The entity compared with the main OBJECT</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>PRIMARY_KEY</name>
          <description>The entity that uniquely identifies the OBJECT</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>LIMIT</name>
          <description>The entity used to limit the ACTION space (e.g. maximum number of OBJECTs to return)</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>false</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>SOURCE</name>
          <description>The origin of an object associated to the component</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>ALGORITHM</name>
          <description>Set of instructions to perform the ACTION</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>false</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>OBJECT</name>
          <description>The object, target or result of an action. It can also reference the system implementing a described functionality</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
    </roles>
    <gridBasedRules>
      <entry>
        <string>SOURCE</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>INSTRUMENT</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>OBJECT</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>ORDERING</string>
        <string>isPlural(&quot;OBJECT&quot;)</string>
      </entry>
      <entry>
        <string>LIMIT</string>
        <string>isPlural(&quot;OBJECT&quot;)</string>
      </entry>
      <entry>
        <string>ALGORITHM</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>PRIMARY_KEY</string>
        <string>!exists(&quot;COMPARISON&quot;)</string>
      </entry>
      <entry>
        <string>COMPARISON</string>
        <string>!exists(&quot;PRIMARY_KEY&quot;)</string>
      </entry>
    </gridBasedRules>
  </thematicGrid>
  <thematicGrid>
    <name>Sending Operations</name>
    <description>A sending operation informs one or many OBJECTs located at one ore many DESTINATIONs about an event. The notification takes place via a MESSAGE in a specific FORMAT. This category is based on VerbNet classes tell-37.2 and send-11.1.</description>
    <verbs>
      <string>port</string>
      <string>update</string>
      <string>send</string>
      <string>ship</string>
      <string>advise</string>
      <string>express</string>
      <string>UPS</string>
      <string>hand</string>
      <string>convey</string>
      <string>forward</string>
      <string>deliver</string>
      <string>transport</string>
      <string>notify</string>
      <string>tell</string>
      <string>dispatch</string>
      <string>transmit</string>
      <string>pass_on</string>
      <string>mail</string>
      <string>transfer</string>
      <string>post</string>
      <string>smuggle</string>
      <string>wire</string>
      <string>slip</string>
      <string>return</string>
      <string>FedEx</string>
      <string>pass</string>
      <string>remind</string>
      <string>acquaint</string>
      <string>inform</string>
      <string>airmail</string>
      <string>apprise</string>
      <string>shunt</string>
      <string>sneak</string>
      <string>shift</string>
    </verbs>
    <roles>
      <entry>
        <thematicRole>
          <name>SOURCE_FORMAT</name>
          <description>The FORMAT of the input(s) of an ACTION</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>PRIMARY_KEY</name>
          <description>The entity that uniquely identifies the OBJECT</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>false</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>OBJECT</name>
          <description>The object, target or result of an action. It can also reference the system implementing a described functionality</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>DESTINATION</name>
          <description>A destination for an object associated with the component</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>true</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>MESSAGE</name>
          <description>Feedback information given by the ACTION in (structured) natural language</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>false</boolean>
      </entry>
      <entry>
        <thematicRole>
          <name>NAME</name>
          <description>Non-numerical identifier of the OBJECT</description>
          <roleScope>BOTH</roleScope>
        </thematicRole>
        <boolean>false</boolean>
      </entry>
    </roles>
    <gridBasedRules>
      <entry>
        <string>MESSAGE</string>
        <string>exists(&quot;DESTINATION&quot;)</string>
      </entry>
      <entry>
        <string>NAME</string>
        <string>!exists(&quot;PRIMARY_KEY&quot;)</string>
      </entry>
      <entry>
        <string>DESTINATION</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>OBJECT</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>SOURCE_FORMAT</string>
        <string>always();</string>
      </entry>
      <entry>
        <string>PRIMARY_KEY</string>
        <string>!exists(&quot;NAME&quot;)</string>
      </entry>
    </gridBasedRules>
  </thematicGrid>
</list>